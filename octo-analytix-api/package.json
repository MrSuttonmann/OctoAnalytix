{
  "name": "cloudflare-workers-openapi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "webpack",
    "watch": "wrangler --config=wrangler.dev.toml dev",
    "deploy:dev": "wrangler --config=wrangler.dev.toml deploy",
    "deploy:prod": "wrangler --config=wrangler.prod.toml deploy",
    "migrate:dev": "wrangler d1 migrations apply --config=wranger.dev.toml octo-db",
    "migrate:prod": "wrangler d1 migrations apply --config=wrangler.prod.toml octo-db"
  },
  "dependencies": {
    "@cloudflare/itty-router-openapi": "^1.0.1",
    "@prisma/adapter-d1": "^5.12.1",
    "@prisma/client": "^5.12.1",
    "auth0-cloudflare-compat": "^4.3.1-1",
    "graphql-request": "^6.1.0",
    "prisma": "^5.12.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@types/node": "^20.5.7",
    "@types/service-worker-mock": "^2.0.1",
    "wrangler": "^3.49.0"
  }
}
